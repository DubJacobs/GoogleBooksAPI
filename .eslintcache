ook.volumeInfo.hasOwnProperty('publishedDate') === false) {\n                book.volumeInfo['publishedDate'] = '0000';\n            }\n\n            else if(book.volumeInfo.hasOwnProperty('imageLinks') === false) {\n                book.volumeInfo['imageLinks'] = { thumbnail: 'https://westsiderc.org/wp-content/uploads/2019/08/Image-Not-Available.png'} \n            }\n\n            return book;\n        })\n        return cleanedData;\n    }\n\n    render() {\n        const sortedBooks = this.state.books.sort((a, b) => {\n            if(this.state.sort === 'Newest') {\n                return parseInt(b.volumeInfo.publishedDate.substring(0,4)) - parseInt(a.volumeInfo.publishedDate.substring(0,4))\n            }\n            else if(this.state.sort === 'Oldest') {\n                return parseInt(a.volumeInfo.publishedDate.substring(0,4)) - parseInt(b.volumeInfo.publishedDate.substring(0,4))\n            }\n        })\n        return (\n            <div>\n                <SearchArea searchBook={this.searchBook} handleSearch={this.handleSearch} handleSort={this.handleSort}/>\n                <BookList books={sortedBooks} />\n            </div>\n        );\n    }\n}\n\nexport default Books;\n","/Users/williamjacobs/bootcamp/trainingroom/reactTraining/google_books_api/src/Components/SearchArea.js",[],"/Users/williamjacobs/bootcamp/trainingroom/reactTraining/google_books_api/src/Components/BookList.js",[],"/Users/williamjacobs/bootcamp/trainingroom/reactTraining/google_books_api/src/Components/BookCard.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":64,"column":58,"nodeType":"41","messageId":"42","endLine":64,"endColumn":60},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"array-callback-return","Array.prototype.sort() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","no-global-assign","no-unsafe-negation"]